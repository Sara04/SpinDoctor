  keywords            = {fractional},
  number              = {2},
  owner               = {jli},
  pages               = {527--575},
  publisher           = {American Mathematical Society},
  timestamp           = {2008.09.23},
  title               = {Well-Posedness and Stabilizability of a Viscoelastic
                         Equation in Energy Space},
  url                 = {http://www.jstor.org/stable/2154987},
  volume              = {345},
  year                = {1994}
}

