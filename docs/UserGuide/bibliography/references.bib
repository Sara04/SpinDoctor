  owner     = {Jing},
  pages     = {10.1097/RLI.0000000000000094},
  timestamp = {2014.11.02},
  title     = {Quantitative Non-Gaussian Diffusion and Intravoxel
               Incoherent Motion Magnetic Resonance Imaging:
               Differentiation of Malignant and Benign Breast Lesions.},
  url       = {http://journals.lww.com/investigativeradiology/Fulltext/publishahead/Quantitative_Non_Gaussian_Diffusion_and_Intravoxel.99381.aspx},
