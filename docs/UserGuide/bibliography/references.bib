@article{macdonald2013,
  abstract  = {The study of reactionâ€“diffusion processes is much
               more complicated on general curved surfaces than on
               standard Cartesian coordinate spaces. Here we show how to
               formulate and solve systems of reactionâ€“diffusion
               equations on surfaces in an extremely simple way, using
               only the standard Cartesian form of differential operators,
               and a discrete unorganized point set to represent the
               surface. Our method decouples surface geometry from the
               underlying differential operators. As a consequence, it
               becomes possible to formulate and solve rather general
               reactionâ€“diffusion equations on general surfaces
               without having to consider the complexities of differential
               geometry or sophisticated numerical analysis. To illustrate
               the generality of the method, computations for surface
               diffusion, pattern formation, excitable media, and
               bulk-surface coupling are provided for a variety of complex
               point cloud surfaces.},
  author    = {Macdonald, Colin B. and Merriman, Barry and Ruuth, Steven
               J.},
  date      = {June 4, 2013},
  doi       = {10.1073/pnas.1221408110},
