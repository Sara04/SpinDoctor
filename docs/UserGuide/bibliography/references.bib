@article{latour1994,
  abstract  = {Packed erythrocytes are ideally suited as a model
               
               system for the study of water diffusion in biological
               tissue, because cell size,
               
               membrane permeability, and extracellular volume fraction
               can be varied
               
               independently. We used a pulsed-field-gradient spin echo
               NMR technique to
               
