  issn      = {0946-672X},
  journal   = {Journal of Trace Elements in Medicine and Biology},
  keywords  = {Alzheimer's disease},
  note      = {Special Section: Iron and Obesity},
  owner     = {Jing},
  pages     = {118 - 123},
  timestamp = {2016.04.12},
  title     = {Age-related changes of brain iron load changes in the
               frontal cortex in APPswe/PS1DE9 transgenic mouse model of
               Alzheimer's disease},
  url       = {http://www.sciencedirect.com/science/article/pii/S0946672X1400251X},
  volume    = {30},
  year      = {2015}
}

