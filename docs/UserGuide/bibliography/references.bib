  timestamp = {2014.11.02},
  title     = {Weighted linear least squares estimation of diffusion MRI
               parameters: Strengths, limitations, and pitfalls},
  url       = {http://www.sciencedirect.com/science/article/pii/S1053811913005223},
  volume    = {81},
  year      = {2013}
}

