  journal   = {Cerebral Cortex},
  number    = {7},
  owner     = {Jing},
  pages     = {990--1001},
  timestamp = {2013.01.20},
  title     = {Dendritic Size of Pyramidal Neurons Differs among Mouse
               Cortical Regions},
  url       = {http://cercor.oxfordjournals.org/content/16/7/990.abstract},
  volume    = {16},
  year      = {July 2006}
}

